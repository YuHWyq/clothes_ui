<link rel="stylesheet" href="/assets/css/ace-rtl.min.css" />
<link rel="stylesheet" href="/assets/css/ace-skins.min.css" />
<script src="/assets/js/ace-extra.min.js"></script>
<style>
    body {
        background-color: #f1f1f0;
    }

    .one_se {
        width: 100%;
        height: 95px;
        /*background-color: #438eb9;*/
        border-radius: 0px 0px 10px 10px;
        font-size: 20px;
        /*color: white;*/
        text-align: center;
        line-height: 95px;
    }

    .two_se {
        float: left;
        width: 210px;
        height: 95px;
        border-radius: 0px 10px 0px 0px;
        font-size: 20px;
        text-align: center;
        line-height: 95px;
    }

    .title {
        width: 300px;
        height: 30px;
        color: #808080;
        font-size: 20px;
        line-height: 30px;
        padding-left: 200px;
    }
</style>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>track</title>
</head>
<body>

    <div class="section">

        <div class="section_box">
            <div id="appbox">
                <div id="app" style="width:1000px;height:500px;">

                </div>
            </div>
        </div>
    </div>


    <script src="~/Scripts/echarts.js"></script>
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scriptsue.min.js"></script>
    <script src="~/Scripts/ElementUI/element-ui.js"></script>
    <script>
        var myChart = echarts.init(document.getElementById('app'));
        option = {
            title: {
                text: '订单统计',
                subtext: '数据来自数据统计'
            },
            tooltip: {
                trigger: 'axis',
                axisPointer: {
                    type: 'shadow'
                }
            },
            legend: {
                data: ['2020年', '2021年']
            },
            grid: {
                left: '3%',
                right: '4%',
                bottom: '3%',
                containLabel: true
            },
            xAxis: {
                type: 'value',
                boundaryGap: [0, 0.01]
            },
            yAxis: {
                type: 'category',
                data: ['7月份', '8月份', '9月份', '10月份', '11月份', '2020-2021总订单统计']
            },
            series: [
                {
                    name: '2020年',
                    type: 'bar',
                    data: [18203, 23489, 29034, 104970, 131744, 630230],
                    color:'#5c7bd9'
                },
                {
                    name: '2021年',
                    type: 'bar',
                    data: [19325, 23438, 31000, 121594, 134141, 681807],
                    color: '#9fe080'
                }
            ]
        };
        myChart.setOption(option);
    </script>
</body>


</html>
