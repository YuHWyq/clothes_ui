
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Productes</title>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/ElementUI/element-ui.js"></script>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script src="~/Scripts/vue.js"></script>
    <link href="~/Content/ElementUI/element-ui.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://unpkg.com/element-ui/lib/theme-chalk/index.css">
    <style>
        .top {
            margin-left: 50px;
        }

        .ss {
            width: 100%;
            height: 80px;
            box-shadow: 0px 0px 3px 3px lightgray;
        }

        .btn1 {
            font-size: 20px;
            width: 80px;
            height: 40px;
            border-radius: 2px;
            color: white;
            background-color: lightseagreen;
            border: none;
        }

        .cc {
            margin-top: 10px;
            width: 100%;
            height: 300px;
            box-shadow: 0px 0px 5px 5px lightgray;
        }

        .yy {
            width: 100%;
            height: 100%;
            box-shadow: 0px 0px 5px 5px lightgray;
        }

        .top1 {
            margin-left: 50px;
        }

        .btn {
            margin-left: 1250px;
        }

        .btn2 {
            font-size: 20px;
            width: 80px;
            height: 40px;
            border-radius: 2px;
            color: white;
            background-color: orange;
            border: none;
        }

        .btn3 {
            font-size: 20px;
            width: 80px;
            height: 40px;
            border-radius: 2px;
            color: white;
            background-color: lightseagreen;
            border: none;
            margin-left: -5px;
        }

        .btn4 {
            font-size: 20px;
            width: 80px;
            height: 35px;
            border-radius: 2px;
            color: white;
            background-color: lightseagreen;
            border: none;
        }

        .text {
            width: 200px;
            height: 40px;
        }

        .btn11 {
            font-size: 20px;
            width: 80px;
            height: 40px;
            border-radius: 2px;
            color: white;
            background-color: lightseagreen;
            border: none;
        }

        .top2 {
            margin-left: 50px;
        }
    </style>
</head>
<body>
    <div id="app">
        <div class="ss">
            <div style="margin-left:50px;">
                <br />
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item" style="font-size:15px;color:dimgray;">产品库存管理</li>
                        <li class="breadcrumb-item" style="font-size:15px;color:dimgray;">添加产品出库单</li>
                    </ol>
                </nav>
            </div>
        </div>
        <br />
        <br />
        <div class="cc">
            <div class="top">
                <span style="font-size:20px;color:dimgray; margin-left:20px;">填写产品出库信息</span>
                <hr />
                <table class="table table-borderless">
                    <thead>
                        <tr>
                            <th scope="col">生产编号：</th>
                            <th scope="col">操作员:</th>
                            <th scope="col">经办人:</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td><input type="text" style="width: 500px; height: 30px;" placeholder="请输入生产编号" /></td>
                            <td><input type="text" style="width: 500px; height: 30px;" placeholder="请输入操作员姓名" /></td>
                            <td><input type="text" style="width: 500px; height: 30px;" placeholder="请输入经办人姓名" /></td>
                        </tr>
                    </tbody>
                </table>
                <div style="width:100%;display:flex;">
                    <div style="width:270px;">
                        <table class="table table-borderless">
                            <thead>
                                <tr>
                                    <th scope="col">出库日期：</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td><input type="date" style="width: 500px; height: 30px;" /></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <span><input type="button" value="提交" class="btn1" style="margin-left:100px;margin-top:40px;" /></span>
                </div>
            </div>
        </div>
        <br />
        <br />
        <div class="yy">
            <div class="top2">
                <br />
                <span style="font-size:20px;color:dimgray;">&nbsp;&nbsp;&nbsp;&nbsp;订单管理</span>
                <span class="btn">
                    <input type="button" value="删除" class="btn2" />
                </span>
                <br />
                <hr />
                <br />
                <span style="display:flex;">
                    <input type="text" class="text" placeholder="请输入订单编号或客户" />
                    <el-button type="primary" icon="el-icon-search">搜索</el-button>
                </span>
                <br />
                <span style="font-size:20px;color:dimgray;display:flex;">
                    显示<input type="text" list="xl" style="width: 80px; height: 22px;" />
                    <datalist id="xl">
                        <option>1</option>
                        <option>50</option>
                        <option>100</option>
                    </datalist>条
                </span>
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th scope="col" style="width:50px;">选择</th>
                            <th scope="col">订单编号</th>
                            <th scope="col">订单客户</th>
                            <th scope="col">订单日期</th>
                            <th scope="col">商品总数量</th>
                            <th scope="col">总金额</th>
                            <th scope="col">预定定金</th>
                            <th scope="col">未付金额</th>
                            <th scope="col">状态</th>
                            <th scope="col">操作</th>
                        </tr>
                    </thead>
                    <tbody>
                        @*@foreach (var item in collection)
                            {

                            }*@
                        <tr @*v-for="item in info"*@>
                            <td><input type="checkbox" /></td>
                            <td>1001</td>
                            <td>蓝思科技</td>
                            <td>2021/7/8</td>
                            <td>3000</td>
                            <td>50000</td>
                            <td>40000</td>
                            <td>10000</td>
                            <td>已处理</td>
                            <td>
                                <input type="button" value="查看" class="btn11" />&nbsp;&nbsp;&nbsp;&nbsp;
                                <input type="button" value="编辑" class="btn11" />
                            </td>
                        </tr>
                    </tbody>
                </table>
                <div style="margin-left:1250px;">
                    <nav aria-label="Page navigation example">
                        <ul class="pagination">
                            <li class="page-item">
                                <a class="page-link" href="#" aria-label="Previous">
                                    <span aria-hidden="true">&laquo;</span>
                                </a>
                            </li>
                            <li class="page-item"><a class="page-link" href="#">1</a></li>
                            <li class="page-item"><a class="page-link" href="#">2</a></li>
                            <li class="page-item"><a class="page-link" href="#">3</a></li>
                            <li class="page-item">
                                <a class="page-link" href="#" aria-label="Next">
                                    <span aria-hidden="true">&raquo;</span>
                                </a>
                            </li>
                        </ul>
                    </nav>
                </div>
            </div>
        </div>
    </div>
</body>
<script src="https://unpkg.com/vue/dist/vue.js"></script>
<!-- import JavaScript -->
<script src="https://unpkg.com/element-ui/lib/index.js"></script>
<script>
    new Vue({
        el: '#app',
        data: function () {
            return { visible: false }
        }
    })
</script>
</html>
