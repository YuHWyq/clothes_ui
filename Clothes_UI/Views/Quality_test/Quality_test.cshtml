
@{
    Layout = null;
}
@model IEnumerable<Clothes_UI.Models.Product_Quality_test_selecte_Result>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Quality_test</title>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <style>
        .yy {
            width: 100%;
            height: 100%;
            box-shadow: 0px 0px 5px 5px lightgray;
        }

        .top1 {
            margin-left: 50px;
        }

        .btn {
            margin-left: 1350px;
        }

        .btn1 {
            font-size: 20px;
            width: 80px;
            height: 40px;
            border-radius: 2px;
            color: white;
            background-color: orange;
            border: none;
        }

        .btn2 {
            font-size: 20px;
            width: 80px;
            height: 40px;
            border-radius: 2px;
            color: white;
            background-color: lightseagreen;
            border: none;
            margin-left: -5px;
        }

        .text {
            width: 200px;
            height: 40px;
        }

        .ss {
            width: 100%;
            height: 80px;
            box-shadow: 0px 0px 3px 3px lightgray;
        }
    </style>
</head>
<body>
    <div>
        <div class="ss">
            <div style="margin-left:50px;">
                <br />
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item" style="font-size:15px;color:dimgray;">质检管理</li>
                        <li class="breadcrumb-item" style="font-size:15px;color:dimgray;">产品质检单</li>
                    </ol>
                </nav>
            </div>
        </div>
        <br />
        <br />
        <div class="yy">
            <div class="top1">
                <br />
                <span style="font-size:20px;color:dimgray;">&nbsp;&nbsp;&nbsp;&nbsp;产品质检管理</span>
                <span class="btn">
                    
                </span>
                <br />
                <hr />
                <br />
                <span style="display:flex;">
                    <input type="text" class="text" placeholder="请输入生产编号/产品名称" />
                    <input type="button" value="搜索" class="btn2" />
                </span>
                <br />
                <table class="table table-bordered">
                    <thead>
                        <tr>
                            <th scope="col">质检编号</th>
                            <th scope="col">生产编号</th>
                            <th scope="col">产品名称</th>
                            <th scope="col">质检日期</th>
                            <th scope="col">操作员</th>
                            <th scope="col">质检结果</th>
                            <th scope="col">操作</th>
                        </tr>
                    </thead>
                    <tbody>

                        @foreach (var item in Model)
                        {
                        <tr>
                            <td>@item.quality_testing_id</td>
                            <td>@item.pro_production_id</td>
                            <td>@item.product_name</td>
                            <td>@item.quality_testing_time</td>
                            <td>@item.operator_per</td>
                            <td>@item.result</td>
                            <td><a href="/Quality_test/Delete?id=@item.quality_testing_id" onclick="return confirm('确认删除嘛')"><input type="button" value="删除" class="btn2" /></a></td>
                        </tr>
                                }
                            
                        </tbody>
                </table>
            </div>
        </div>
    </div>
</body>
</html>
